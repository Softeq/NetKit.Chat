{
  "ConnectionStrings": {
    "DefaultConnection": "[connection string to sql server database]"
  },
  "AzureStorage": {
    "ConnectionString": "connection string to azure storage",
    "ContentStorageHost": "[content storage url]",
    "MessageAttachmentsContainer": "message",
    "MemberAvatarsContainer": "avatar",
    "ChannelImagesContainer": "channel",
    "TempContainerName": "temp",
    "MessagePhotoSize": 300
  },
  "Authentications": {
    "Bearer": {
      "ApiSecret": "secret",
      "Authority": "[authority url]",
      "RequireHttpsMetadata": false,
      "ApiName": "api"
    }
  },
  "MessageAttachments": {
    "Limit": 10
  },
  "ApplicationInsights": {
    "InstrumentationKey": "[your InstrumentationKey]"
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "ApplicationName": "[application name]",
    "FileSizeLimitMBytes": 100,
    "EnableLocalFileSink": "True"
  }
}